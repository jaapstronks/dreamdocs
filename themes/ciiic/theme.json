{
  "name": "CIIIC",
  "description": "Professional theme with GT America typography, lime yellow accents and teal highlights",
  "version": "1.0.0",
  "author": "CIIIC",
  "private": true,
  "fonts": [
    {
      "family": "GT America",
      "weight": 400,
      "style": "normal",
      "src": "fonts/GT-America-LCGV-Standard-Regular.ttf"
    },
    {
      "family": "GT America",
      "weight": 700,
      "style": "normal",
      "src": "fonts/GT-America-LCGV-Standard-Bold.ttf"
    },
    {
      "family": "GT America",
      "weight": 400,
      "style": "italic",
      "src": "fonts/GT-America-LCGV-Standard-Regular-Italic.ttf"
    },
    {
      "family": "GT America",
      "weight": 700,
      "style": "italic",
      "src": "fonts/GT-America-LCGV-Standard-Bold-Italic.ttf"
    },
    {
      "family": "GT America Extended",
      "weight": 500,
      "style": "normal",
      "src": "fonts/GT-America-LCGV-Extended-Medium.ttf"
    },
    {
      "family": "JetBrains Mono",
      "weight": 400,
      "style": "normal",
      "src": "fonts/JetBrainsMono-Regular.ttf"
    }
  ],
  "colors": {
    "text": "#141414",
    "heading": "#141414",
    "accent": "#dbff00",
    "primary": "#38766d",
    "link": "#38766d",
    "link-hover": "#2c5f57",
    "code-bg": "#f6f8fa",
    "code-text": "#d6336c",
    "border": "#ddd",
    "muted": "#777777",
    "toc-bg": "#f5f5f5",
    "callout-bg": "#e0e5e2",
    "callout-border": "#dbff00"
  },
  "pageSettings": {
    "format": "A4",
    "margins": {
      "top": "2cm",
      "right": "2cm",
      "bottom": "2cm",
      "left": "2cm"
    }
  }
}
